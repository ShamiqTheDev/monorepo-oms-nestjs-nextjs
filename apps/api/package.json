{
  "name": "api",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "dotenv -e .env.local -- vite build --mode=production",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/main.js",
    "dev": "dotenv -e .env.local -- vite",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@atdb/eslint-config": "workspace:*",
    "@atdb/tsconfig": "workspace:*",
    "@swc/core": "^1.3.92",
    "@types/node": "^17.0.45",
    "dotenv-cli": "^7.3.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.3",
    "vite-plugin-node": "^3.0.2"
  },
  "dependencies": {
    "@atdb/server-auth": "workspace:*",
    "@atdb/server-categories": "workspace:^",
    "@atdb/server-config": "workspace:*",
    "@atdb/server-invitations": "workspace:^",
    "@atdb/server-kysely": "workspace:*",
    "@atdb/server-notifications": "workspace:^",
    "@atdb/server-order-changes": "workspace:^",
    "@atdb/server-orders": "workspace:*",
    "@atdb/server-patients": "workspace:^",
    "@atdb/server-settings": "workspace:^",
    "@atdb/server-sub-categories": "workspace:^",
    "@atdb/server-users": "workspace:*",
    "@atdb/types": "workspace:^",
    "@nestjs/common": "^10.2.7",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@node-rs/argon2": "^1.5.2",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.4",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "helmet": "^7.0.0",
    "nestjs-pino": "^3.5.0",
    "pino-pretty": "^10.2.3",
    "reflect-metadata": "^0.1.13"
  }
}
